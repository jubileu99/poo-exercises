
public class FiboTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int n = 9, anterior = 0, atual = 0;
		
		for(int i = 1; i < n ; i++) {
			if(i == 1) {
				atual = 1;
				anterior = 0;
			}else {
				atual += anterior;
				anterior = atual-anterior;
			}
		}
		System.out.println(" "+atual);
		
	}

}

// 0 1 1 2 3 5 8 13 21

// f(n) = f(n-1) + f(n-2)
