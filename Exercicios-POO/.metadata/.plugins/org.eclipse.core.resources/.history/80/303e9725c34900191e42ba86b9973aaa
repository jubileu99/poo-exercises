
public class Fibo {
	
	private int n;
	
	public Fibo(int n) {
		this.n = n;
	}
	
	public int[] PutFiboInArray(int n) {
		
		int[] FiboArray = new int[n];
		int anterior = 0, atual = 0;
		
		for(int i = 0; i < n ; i++) {
			if(i == 1) {
				atual = 1;
				anterior = 0;
			}else {
				atual = atual + anterior;
				anterior = atual - anterior;
			}
			System.out.println(atual);
		}
		
		return FiboArray;
	}
	
	public void somatorio(int a, int b) {
		
	}
	
}
